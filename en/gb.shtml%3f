function checkForm(form)
{
  if (form.name.value == '')
  {
    alert("You must specify your name!");
    form.name.focus();
    return false;
  }
  if (form.subj.value == '')
  {
    alert("You must specify message subject!");
    form.subj.focus();
    return false;
  }
  if (form.body.value == '')
  {
    alert("You must specify message body!");
    form.body.focus();
    return false;
  }
  return true;
}
//-->
